{
	"compilerOptions": {
		"baseUrl": "./",
		"experimentalDecorators": true,
		"jsx": "react",
		"lib": [
			"es6",
			"dom"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": "Target",
		"rootDir": "Source",
		"sourceMap": true,
		"strict": true,
		"target": "es6"
	},
	"exclude": [
		"node_modules"
	],
	"extends": "@playform/build/tsconfig",
	"include": [
		"Source"
	]
}

    "gulp-cli": "2.3.0",
    "gulp-filter": "9.0.1",
    "gulp-sourcemaps": "3.0.0",
    "gulp-typescript": "5.0.1",
    "less": "4.2.0",
    "less-loader": "12.1.0",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5",
    "react-scripts": "5.0.1",
    "style-loader": "3.3.4",
    "ts-import-plugin": "3.0.0",
    "ts-loader": "9.5.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.1.0",
    "tslint-react": "4.1.0",
    "tslint-react-hooks": "2.2.2",
    "typescript": "5.3.3",
    "typescript-esbuild": "0.3.3",
    "typescript-react-intl": "0.4.1",
